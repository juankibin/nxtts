{
  "extends": "next/core-web-vitals",
  "require-jsdoc": 0,
  "spaced-comment": [
    "error",
    "always",
    {
      "line": {
        "markers": ["/"],
        "exceptions": ["-", "+"]
      },
      "block": {
        "markers": ["!"],
        "exceptions": ["*"],
        "balanced": true
      }
    }
  ]
}
